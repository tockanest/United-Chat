{
      "productName": "united-chat",
            "version": "0.1.0",
            "identifier": "nixyan.united-chat",
      "build": {
            "beforeDevCommand": "npm run dev",
            "beforeBuildCommand": "npm run build",
            "devUrl": "http://localhost:3000",
            "frontendDist": "../out"
      },
      "app": {
            "withGlobalTauri": true,
            "windows": [
                  {
                      "title": "United Chat",
                    "width": 800,
                    "height": 600
                  }
            ],
            "security": {
                "csp": null
            }
      },
      "bundle": {
            "active": true,
            "targets": "all",
            "icon": [
                "icons/256x256.png",
                "icons/256x256.ico"
            ]
      }
}
